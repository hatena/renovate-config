{
  "regexManagers": [
    {
      "fileMatch": [
        "^*Dockerfile$"
      ],
      "matchStrings": [
        "FROM node:(?<currentValue>[0-9.]+?)[^0-9.]*\\n"
      ],
      "depNameTemplate": "node",
      "datasourceTemplate": "node",
      "versioningTemplate": "node"
    }
  ],
  "packageRules": [
    {
      "matchPackageNames": [
        "node"
      ],
      "matchManagers": [
        "dockerfile"
      ],
      "enabled": false
    }
  ]
}